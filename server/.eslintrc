{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "func-names": 0,
    "prefer-const": 0,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "dot-notation": 0,
    "id-length": 0,
    "no-undef": 0,
    "no-var": 0,
    "new-cap": 0,
    "indent": [2, 2, { "SwitchCase": 1 }]
  }
}
